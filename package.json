{
  "name": "webpack-stats-explorer",
  "version": "1.2.0",
  "description": "Tool for exploring webpack stats and comparing them between builds",
  "repository": {
    "type": "git",
    "url": "git@github.com:erykpiast/webpack-stats-explorer.git"
  },
  "bugs": {
    "url": "https://github.com/erykpiast/webpack-stats-explorer/issues"
  },
  "homepage": "https://github.com/erykpiast/webpack-stats-explorer",
  "scripts": {
    "clean": "rm -rf dist",
    "precompile": "bsb -clean-world",
    "compile": "bsb -make-world",
    "prebuild": "run-s compile clean",
    "build": "NODE_ENV=production webpack",
    "start": "bsb -make-world -w",
    "pretest": "npm run compile",
    "test": "TEST=true jest",
    "webpack": "webpack-dev-server",
    "format": "find src -name '*.ml' -exec xargs ocamlformat -i -p janestreet {} \\;",
    "release": "standard-version"
  },
  "keywords": [
    "BuckleScript"
  ],
  "author": "Eryk Napiera≈Ça <eryk.piast@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@glennsl/bs-json": "3.0.0",
    "@glennsl/rebase": "0.2.1",
    "@zpnester/bs-filereader": "0.2.4",
    "bs-css": "8.0.4",
    "bs-jsverify": "0.10.0",
    "bs-react-dropzone": "0.3.3",
    "diff": "4.0.1",
    "rationale": "0.1.10",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-dropzone": "10.1.4",
    "reason-react": "0.7.0"
  },
  "devDependencies": {
    "@babel/cli": "7.4.3",
    "@babel/core": "7.4.3",
    "@babel/preset-env": "7.4.3",
    "@glennsl/bs-jest": "0.4.8",
    "babel-jest": "24.5.0",
    "bs-enzyme": "0.6.1",
    "bs-platform": "5.0.3",
    "enzyme-adapter-react-16": "1.12.1",
    "gh-pages": "2.0.1",
    "html-webpack-plugin": "3.2.0",
    "jest": "24.5.0",
    "jsverify": "0.8.4",
    "npm-run-all": "4.1.5",
    "standard-version": "5.0.2",
    "webpack": "4.30.0",
    "webpack-cli": "3.3.1",
    "webpack-dev-server": "3.3.1"
  }
}
