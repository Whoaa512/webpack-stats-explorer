{
  "name": "webpack-stats-explorer",
  "version": "1.0.0",
  "description": "Tool for exploring webpack stats and comparing them between builds",
  "repository": {
    "type": "git",
    "url": "git@github.com:erykpiast/webpack-stats-explorer.git"
  },
  "bugs": {
    "url": "https://github.com/erykpiast/webpack-stats-explorer/issues"
  },
  "homepage": "https://github.com/erykpiast/webpack-stats-explorer",
  "scripts": {
    "prebuild": "npm run clean",
    "build": "bsb -make-world",
    "start": "bsb -make-world -w",
    "clean": "bsb -clean-world",
    "pretest": "npm run build",
    "test": "TEST=true jest",
    "webpack": "webpack-dev-server",
    "webpack:production": "NODE_ENV=production webpack",
    "format": "find src -name '*.ml' -exec xargs ocamlformat -i -p janestreet {} \\;"
  },
  "keywords": [
    "BuckleScript"
  ],
  "author": "Eryk Napiera≈Ça <eryk.piast@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@glennsl/bs-json": "3.0.0",
    "@glennsl/rebase": "0.2.1",
    "bs-css": "8.0.2",
    "bs-jsverify": "0.10.0",
    "bs-react-dropzone": "0.1.0",
    "diff": "4.0.1",
    "rationale": "0.1.10",
    "react": "16.8.4",
    "react-dom": "16.8.4",
    "react-dropzone": "10.0.0",
    "reason-react": "0.5.3"
  },
  "devDependencies": {
    "@babel/cli": "7.2.3",
    "@babel/core": "7.3.4",
    "@babel/preset-env": "7.3.4",
    "@glennsl/bs-jest": "0.4.8",
    "babel-jest": "24.3.1",
    "bs-enzyme": "0.6.1",
    "bs-platform": "4.0.18",
    "enzyme-adapter-react-16": "1.10.0",
    "html-webpack-plugin": "3.2.0",
    "jest": "24.3.1",
    "jsverify": "0.8.4",
    "npm-run-all": "4.1.5",
    "webpack": "4.29.6",
    "webpack-cli": "3.2.3",
    "webpack-dev-server": "3.2.1"
  }
}
